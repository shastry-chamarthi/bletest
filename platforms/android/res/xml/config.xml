<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gtk.bletest" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>BLE Flash light</name>
    <description>
        BLE Camera Test
    </description>
    <author href="sathyanarayanasastry@gmail.com">
        Sathya Chamarthi
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="Flashlight">
        <param name="android-package" value="nl.xservices.plugins.Flashlight" />
    </feature>
</widget>
